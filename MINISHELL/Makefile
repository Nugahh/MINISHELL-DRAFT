#------  1.PROJECT FILES VARIABLES

NAME = minishell

MFILES = \
		./PARSING/first_split_index.c		\
		main.c


OBJS :=  	${addprefix ./obj/,${MFILES:%.c=%.o}}
DEP   = 	${addprefix ./obj/,${OBJS:%.o=%.d}}

#------  2.COMPILING VARIABLES

CFLAGS = -Wall -Werror -Wextra -MMD -MP -I./includes -g3 
RL = -lreadline
LIB	   = -L./LIB/libft/ -lft

#------  3.COLORS VARIABLES

RESET=        \033[0m
DARK=        \033[1;32m
RED=        \033[31m
GREEN=        \033[32m
YELLOW=        \033[33m
BLUE=        \033[34m
MAGENTA=    \033[35m
CYAN=        \033[36m
WHITE=        \033[37m
BOLDBLACK=    \033[1m\033[30m
BOLDRED=    \033[1m\033[31m
BOLDWHITE=    \033[1m\033[37m
BOLDGREEN=    \033[1m\033[32m

#------  4.RULES

all: ${NAME}
-include ${DEP}

./obj/%.o: ./%.c
	@mkdir -p ${@D}
	@make -C ./LIB/libft/
	@cc ${CFLAGS} -o $@ -c $<
	
